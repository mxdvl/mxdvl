{
	"redirects": [
		{
			"source": "/",
			"has": [{ "type": "header", "key": "Accept-Language", "value": "fr.+" }],
			"destination": "/allo",
			"permanent": false
		},
		{
			"source": "/",
			"destination": "/hi",
			"permanent": false
		},
		{
			"source": "/:erreur+",
			"has": [{ "type": "header", "key": "Accept-Language", "value": "fr.+" }],
			"destination": "/erreur",
			"permanent": false
		},
		{
			"source": "/:error+",
			"destination": "/error",
			"permanent": false
		}
	]
}

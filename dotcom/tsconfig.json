{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"noUncheckedIndexedAccess": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"],
			"$types/*": [".svelte-kit/types/*"],
			"@mxdvl/content/*": ["../content/src/*"],
			"@mxdvl/dotcom/*": ["../dotcom/src/*"],
			"@mxdvl/config/*": ["../config/src/*"]
		}
	}
}

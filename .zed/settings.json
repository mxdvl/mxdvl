{
  "languages": {
    "JavaScript": {
      "language_servers": [
        "deno",
        "!typescript-language-server",
        "!vtsls",
        "!eslint",
        "..."
      ],
      "format_on_save": {
        "external": {
          "command": "deno",
          "arguments": [
            "fmt",
            "-"
          ]
        }
      }
    },
    "TypeScript": {
      "language_servers": [
        "deno",
        "!typescript-language-server",
        "!vtsls",
        "!eslint",
        "..."
      ],
      "format_on_save": {
        "external": {
          "command": "deno",
          "arguments": [
            "fmt",
            "-"
          ]
        }
      }
    }
  }
}
